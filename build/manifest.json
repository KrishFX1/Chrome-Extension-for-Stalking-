{
  "manifest_version": 2,
  "name": "WhatsApp-Spam",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "history"
  ],
  "icons": {
    "16": "./icons/icon_16.png",
    "48": "./icons/icon_48.png",
    "128": "./icons/icon_128.png"
  }
}